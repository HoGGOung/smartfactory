namespace ConsoleApp43
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int[] num = new int[6];
            for (int i = 0; i < 6; i++)
            {
                num[i] = random.Next(1, 47);
                Console.Write(num[i]+" ");
            }
            Console.WriteLine();
            int num2 = random.Next(1, 47);
            Console.WriteLine(num2);
        } 
    }
}
