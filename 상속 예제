namespace ConsoleApp26
{
    class Computer
    {
        public void Run()
        {
            Console.WriteLine("컴퓨터를 구동합니다");
        }
    }
    class Notebook : Computer
    {

    }
    internal class Program
    {
        static void Main(string[] args)
        {
            Computer computer = new Computer();
            computer.Run();
            Notebook notebook = new Notebook();
            notebook.Run();
        }
    }
}
