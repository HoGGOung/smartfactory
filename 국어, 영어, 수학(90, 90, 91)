namespace score02
{
    internal class Program
    {
        static int TotalScore(int a, int b, int c)
        {
            int score = 0;
            score = a + b + c;
            return score;
        }
        static void Main(string[] args)
        {
            Console.WriteLine(TotalScore(90, 90, 91));
        }
    }
}
